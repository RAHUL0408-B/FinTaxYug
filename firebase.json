{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "apphosting": {
      "port": 5001,
      "rootDirectory": "./",
      "startCommand": "npm run dev"
    },
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5002
    },
    "firestore": {
      "port": 5003
    },
    "database": {
      "port": 5004
    },
    "hosting": {
      "port": 5005
    },
    "pubsub": {
      "port": 5006
    },
    "storage": {
      "port": 5007
    },
    "eventarc": {
      "port": 5008
    },
    "dataconnect": {
      "port": 5009,
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    },
    "tasks": {
      "port": 5010
    },
    "ui": {
      "enabled": true,
      "port": 5011
    },
    "singleProjectMode": true
  }
}